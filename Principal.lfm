							<script>
								local function interpretarRolagemExMag()
									local mesaDoPersonagem = rrpg.getMesaDe(sheet);
										sheet.ExecucaoMagicaTeste = sheet.ExecucaoMagica or 0;
										mesaDoPersonagem.chat:rolarDados("1d20 + " .. math.floor(sheet.ExecucaoMagica), "[§K3]Execução Magica!",
											
										function (rolado)
											if sheet.CDEI == nil then
											showMessage("Você não especificou o CDEI|Classe de esquiva do inimigo|")
											end;
											if sheet.CDBI == nil then
												if rolado.resultado >= sheet.CrtMagic then
													mesaDoPersonagem.chat:enviarMensagem("[§K4]DANO CRITICO = " .. rolado.resultado);
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Corporal = " .. "[§K4]" .. math.floor(sheet.DanoCorporal) * (sheet.PowerFgt) .. "[§K1] ou " .. math.floor(sheet.DanoCorporalx) * (sheet.PowerFgt) ) ;
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Magico = " .. "[§K4]" .. math.floor(sheet.DanoMagico) * (sheet.PowerMag) .. "[§K1] ou " .. math.floor(sheet.DanoMagicox) * (sheet.PowerMag) ) ;
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Mira = " .. "[§K4]" .. math.floor(sheet.DanoMira) * (sheet.PowerFgt) .. "[§K1] ou " .. math.floor(sheet.DanoMirax) * (sheet.PowerFgt) ) ;
												   
											
												elseif rolado.resultado >= sheet.CDEI then
													mesaDoPersonagem.chat:enviarMensagem("[§K4]" .. sheet.Nome .. " Atacou!");
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Corporal = " .. "[§K4]" .. math.floor(sheet.DanoCorporal) .. "[§K1] ou " .. math.floor(sheet.DanoCorporalx) ) ;
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Magico = " .. "[§K4]" .. math.floor(sheet.DanoMagico) .. "[§K1] ou " .. math.floor(sheet.DanoMagicox) ) ;
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Mira = " .. "[§K4]" .. math.floor(sheet.DanoMira) .. "[§K1] ou " .. math.floor(sheet.DanoMirax) ) ;
												   
													mesaDoPersonagem.chat:enviarMensagem("[§K3]Precisa Tirar = " .. sheet.CDEI .. "[§K3] ou Mais");
												   
												else
													mesaDoPersonagem.chat:enviarMensagem("[§K4]Na sua mente você acertou " .. "[§K9]" .. rolado.resultado);
													mesaDoPersonagem.chat:enviarMensagem("[§I" .. "https://memesengracados.net/wp-content/uploads/2020/09/gato-triste-variacao-1.png" .. "]");
												end;
											end;
											if sheet.CDBI ~= nil then
												if rolado.resultado >= sheet.CrtMagic then
													mesaDoPersonagem.chat:enviarMensagem("[§K4]DANO CRITICO = " .. rolado.resultado);
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Corporal = " .. "[§K4]" .. math.floor(sheet.DanoCorporal) * (sheet.PowerFgt) .. "[§K1] ou " .. math.floor(sheet.DanoCorporalx) * (sheet.PowerFgt) ) ;
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Magico = " .. "[§K4]" .. math.floor(sheet.DanoMagico) * (sheet.PowerMag) .. "[§K1] ou " .. math.floor(sheet.DanoMagicox) * (sheet.PowerMag) ) ;
													mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Mira = " .. "[§K4]" .. math.floor(sheet.DanoMira) * (sheet.PowerFgt) .. "[§K1] ou " .. math.floor(sheet.DanoMirax) * (sheet.PowerFgt) ) ;
												   
												elseif rolado.resultado >= sheet.CDBI then
														mesaDoPersonagem.chat:enviarMensagem("[§K4]" .. sheet.Nome .. " Atacou!");
														mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Corporal = " .. "[§K4]" .. math.floor(sheet.DanoCorporal) .. "[§K1] ou " .. math.floor(sheet.DanoCorporalx) ) ;
														mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Magico = " .. "[§K4]" .. math.floor(sheet.DanoMagico) .. "[§K1] ou " .. math.floor(sheet.DanoMagicox) ) ;
														mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Mira = " .. "[§K4]" .. math.floor(sheet.DanoMira) .. "[§K1] ou " .. math.floor(sheet.DanoMirax) ) ;
												   
												elseif rolado.resultado >= sheet.CDEI then
														mesaDoPersonagem.chat:enviarMensagem("[§I" .. "https://www.gamefm.com.br/wp-content/uploads/2014/05/sta.jpg" .. "]");
														mesaDoPersonagem.chat:enviarMensagem("[§K4]O inimigo bloqueou!");
														mesaDoPersonagem.chat:enviarMensagem("[§K3]Precisava Tirar = " .. sheet.CDBI .. "[§K3] ou Mais");
														mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Corporal = " .. "[§K4]" .. math.floor(sheet.DanoCorporal) .. "[§K1] ou " .. math.floor(sheet.DanoCorporalx) ) ;
														mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Magico = " .. "[§K4]" .. math.floor(sheet.DanoMagico) .. "[§K1] ou " .. math.floor(sheet.DanoMagicox) ) ;
														mesaDoPersonagem.chat:enviarMensagem("[§U]Dano Mira = " .. "[§K4]" .. math.floor(sheet.DanoMira) .. "[§K1] ou " .. math.floor(sheet.DanoMirax) ) ;
												   
													
												else
													mesaDoPersonagem.chat:enviarMensagem("[§K4]Na sua mente você acertou " .. "[§K9]" .. rolado.resultado);
													mesaDoPersonagem.chat:enviarMensagem("[§I" .. "https://memesengracados.net/wp-content/uploads/2020/09/gato-triste-variacao-1.png" .. "]");
												end;
											end;
									end);
									
								end;
